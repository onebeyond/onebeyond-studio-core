{
  "FileStorage": {
    "AzureStorageBlobs": {
      "ConnectionString": "UseDevelopmentStorage=true",
      "ContainerName": "fm-app-files"
    },
    "MimeTypeValidation": {
      "ValidationMode" : "Whitelist",
      "MimeTypeSignatures": [
        {
          "MimeType": "image/png",
          "Signatures": [ "AABBcc", "09DDEE" ]
        },
        {
          "MimeType": "image/jpeg",
          "Signatures": [ "0003" ]
        }
      ]
    }
  }
}